                    android:text="Edit Placement"
                    android:layout_centerVertical="true"
                    android:gravity="center"
                    android:textColor="#282f35"
                    android:id="@+id/editnotificationtxt2"
                    />
            </RelativeLayout>

        </RelativeLayout>
    </RelativeLayout>
