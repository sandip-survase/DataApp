            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:text="Notifications"
            android:textColor="@android:color/holo_blue_bright"
            android:id="@+id/createnotitxt"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/createnotitxt"
            android:textColor="#bbb"
            android:textSize="12dp"
            android:text="Your notifications will be broadcasted only to the students and/or alumnis under your supervision."
            android:layout_marginTop="5dp"
            android:id="@+id/createnotinotitxt"
            />
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/admincontrolsrl"
            android:background="@drawable/boxborder"
            android:layout_below="@+id/createnotinotitxt"
            >

            <View
                android:layout_width="3dp"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:id="@+id/section"
                android:background="#282f35"
                />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_toLeftOf="@+id/section"
                android:id="@+id/createnotificationrl"
                >

                <ImageView
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:src="@drawable/adminplus"
                    android:layout_centerVertical="true"
                    android:id="@+id/addnotification"
                    android:tint="#282f35"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/addnotification"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:text="Create Notification"
                    android:layout_centerVertical="true"
                    android:gravity="center"
                    android:textColor="#282f35"
                    android:id="@+id/createnotificationtxt"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_toRightOf="@+id/section"
                android:id="@+id/editnotificationrl"
                >
                <ImageView
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:src="@drawable/adminedit"
                    android:layout_centerVertical="true"
                    android:id="@+id/editnotification"
                    android:tint="#282f35"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
                    />
