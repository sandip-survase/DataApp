                    android:layout_height="22dp"
                    android:src="@drawable/adminedit"
                    android:layout_centerVertical="true"
                    android:id="@+id/editnotification2"
                    android:tint="#282f35"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
