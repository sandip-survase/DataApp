                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_above="@+id/agreementicon"
                android:layout_below="@+id/settingsline9"

                />
            <View
                android:id="@+id/agreementselectionview"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_above="@+id/signouticon"
                android:layout_below="@+id/settingsline10"

                />

            <View
