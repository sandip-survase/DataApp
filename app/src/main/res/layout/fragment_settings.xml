                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_below="@+id/settingsline11"
                android:layout_above="@+id/settingsline13"
                />


            <ImageView
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/faq"
                android:id="@+id/faqicon"

                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="FAQs"
                android:textSize="16dp"
                android:textColor="#eee"
                android:id="@+id/faqtxt"
                android:layout_marginTop="15dp"
                android:layout_toRightOf="@+id/faqicon"

                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#c59a6d"
                android:layout_marginTop="13dp"
                android:layout_below="@+id/faqicon"
                android:id="@+id/settingsline7"
                />
            <ImageView
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/help"
                android:id="@+id/helpicon"
                android:layout_below="@+id/settingsline7"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Help Center"
                android:textSize="16dp"
                android:textColor="#eee"
                android:id="@+id/helptxt"
                android:layout_marginTop="13dp"
                android:layout_toRightOf="@+id/faqicon"
                android:layout_below="@+id/settingsline7"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#c59a6d"
                android:layout_marginTop="13dp"
                android:layout_below="@+id/helpicon"
                android:id="@+id/settingsline8"
                />

            <ImageView
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/privacypolicy"
                android:id="@+id/privacyicon"
                android:layout_below="@+id/settingsline8"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Privacy Policy"
                android:textSize="16dp"
                android:textColor="#eee"
                android:id="@+id/privacytxt"
                android:layout_marginTop="13dp"
                android:layout_toRightOf="@+id/faqicon"
                android:layout_below="@+id/settingsline8"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#c59a6d"
                android:layout_marginTop="13dp"
                android:layout_below="@+id/privacyicon"
                android:id="@+id/settingsline9"
                />

            <ImageView
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/termsnconditions"
                android:id="@+id/termsicon"
                android:layout_below="@+id/settingsline9"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Terms and Conditions"
                android:textSize="16dp"
                android:textColor="#eee"
                android:id="@+id/termstxt"
                android:layout_marginTop="13dp"
                android:layout_toRightOf="@+id/faqicon"
                android:layout_below="@+id/settingsline9"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#c59a6d"
                android:layout_marginTop="13dp"
                android:layout_below="@+id/termsicon"
                android:id="@+id/settingsline10"
                />

            <ImageView
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/useragreement"
                android:id="@+id/agreementicon"
                android:layout_below="@+id/settingsline10"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="User Agreement"
                android:textSize="16dp"
                android:textColor="#eee"
                android:id="@+id/agreementtxt"
                android:layout_marginTop="13dp"
                android:layout_toRightOf="@+id/faqicon"
                android:layout_below="@+id/settingsline10"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#c59a6d"
                android:layout_marginTop="13dp"
                android:layout_below="@+id/agreementicon"
                android:id="@+id/settingsline11"/>

            <ImageView
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/logout"
                android:id="@+id/signouticon"
                android:layout_below="@+id/settingsline11"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
