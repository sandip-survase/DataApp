<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#282f35"

    >
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/activity_my_profile_intro"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusableInTouchMode="true"
        >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:padding="16dp"
            android:id="@+id/completepersonalrl"
            android:layout_marginTop="30dp"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/studentBlock"
                android:background="@drawable/boxborderwhite"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/knownlangtxt"
                android:text="Known Languages"
                android:textColor="#eee"
                android:textSize="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                />
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:src="@drawable/plus"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/studentBlock"
                android:id="@+id/alumniBlock"
                android:layout_marginTop="20dp"
                android:background="@drawable/boxborderwhite"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/studentBlock"
                android:text="Expert Skills"
                android:textColor="#eee"
                android:id="@+id/skillstxt"
                android:textSize="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="30dp"
                />
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_below="@+id/studentBlock"
                android:layout_alignParentRight="true"
                android:src="@drawable/plus"
                android:layout_marginRight="10dp"
                android:layout_marginTop="30dp"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/alumniBlock"
                android:id="@+id/adminBlock"
                android:layout_marginTop="20dp"
                android:background="@drawable/boxborderwhite"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/alumniBlock"
                android:text="Honors and Awards"
                android:textColor="#eee"
                android:id="@+id/honortxt"
                android:textSize="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="30dp"
                />
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_below="@+id/alumniBlock"
                android:layout_alignParentRight="true"
                android:src="@drawable/plus"
                android:layout_marginRight="10dp"
                android:layout_marginTop="30dp"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/adminBlock"
                android:id="@+id/patentbutton"
                android:layout_marginTop="20dp"
                android:background="@drawable/boxborderwhite"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/adminBlock"
                android:text="Patents"
                android:textColor="#eee"
                android:id="@+id/patenttxt"
                android:textSize="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="30dp"
                />
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_below="@+id/adminBlock"
                android:layout_alignParentRight="true"
                android:src="@drawable/plus"
                android:layout_marginRight="10dp"
                android:layout_marginTop="30dp"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/patentbutton"
                android:id="@+id/publicationbutton"
                android:layout_marginTop="20dp"
                android:background="@drawable/boxborderwhite"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/patentbutton"
                android:text="Publications"
                android:id="@+id/publicationtxt"
                android:textColor="#eee"
                android:textSize="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="30dp"
                />
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_below="@+id/patentbutton"
                android:layout_alignParentRight="true"
                android:src="@drawable/plus"
                android:layout_marginRight="10dp"
                android:layout_marginTop="30dp"
                />

        </RelativeLayout>

    </RelativeLayout>
</android.support.v4.widget.NestedScrollView>
